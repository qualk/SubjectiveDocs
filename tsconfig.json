{
    "extends": "@docusaurus/tsconfig",
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "allowJs": true,
        "checkJs": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "strict": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/",
        "dist/",
        "build/"
    ]
}
